package test_0716;

import java.util.Stack;

public class stack {
  public static void main(String[] args) {
    
    // LIFO 후입 선출
    Stack<String> stack = new Stack<>();
    
    System.out.println(stack);
    stack.push("1번");
    System.out.println(stack);
    stack.push("2번");
    System.out.println(stack);
    stack.push("3번");
    System.out.println(stack);
    stack.push("4번");
    System.out.println(stack);
    stack.push("5번");
    
    System.out.println(stack);
    System.out.println(stack.pop());
    System.out.println(stack);
    System.out.println(stack.pop());
    System.out.println(stack);
    System.out.println(stack.pop());
    System.out.println(stack);
    System.out.println(stack.pop());
    System.out.println(stack);
    System.out.println(stack.pop());
    System.out.println(stack);
    
    
  }
}
